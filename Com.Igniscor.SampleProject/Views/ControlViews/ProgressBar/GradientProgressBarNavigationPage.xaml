<?xml version="1.0" encoding="utf-8"?>

<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:helpers="clr-namespace:SkiaSharpControls.Helpers"
    xmlns:viewModels="clr-namespace:SkiaSharpControls.ViewModels"
    xmlns:support1="clr-namespace:SkiaSharpControls.Views.Support"
    x:Class="SkiaSharpControls.Views.ControlViews.ProgressBar.GradientProgressBarNavigationPage"
    x:DataType="viewModels:GradientProgressBarNavigationViewModel"
    Title="{x:Static helpers:Constants+Texts.ProgressBars}">

    <support1:NavigationButtonListView 
        ItemsSource="{Binding Items}"
        Command="{Binding GoToCommand}"/>
</ContentPage>